{
  "env": {
    "node": true,
    "es6": true,
    "mocha": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "eqeqeq": ["error", "always"],
    "no-console": "off"
  },
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
  },
  "globals": {
    "fixture": true,
    "expect": true,
    "window": true,
    "YT": true
  }
}
